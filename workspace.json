{
  "version": 2,
  "cli": { "defaultCollection": "@nrwl/next" },
  "generators": {
    "@nrwl/react": { "application": { "babel": true } },
    "@nrwl/next": { "application": { "style": "scss", "linter": "eslint" } }
  },
  "defaultProject": "stop-game-next",
  "projects": {
    "stop-game-next": {
      "root": "apps/stop-game-next",
      "sourceRoot": "apps/stop-game-next",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/next:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "root": "apps/stop-game-next",
            "outputPath": "dist/apps/stop-game-next"
          },
          "configurations": { "production": {} }
        },
        "serve": {
          "executor": "@nrwl/next:server",
          "options": { "buildTarget": "stop-game-next:build", "dev": true },
          "configurations": {
            "production": {
              "buildTarget": "stop-game-next:build:production",
              "dev": false
            }
          }
        },
        "export": {
          "executor": "@nrwl/next:export",
          "options": { "buildTarget": "stop-game-next:build:production" }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/stop-game-next/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/stop-game-next"],
          "options": {
            "jestConfig": "apps/stop-game-next/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "stop-game-next-e2e": {
      "root": "apps/stop-game-next-e2e",
      "sourceRoot": "apps/stop-game-next-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/stop-game-next-e2e/cypress.json",
            "tsConfig": "apps/stop-game-next-e2e/tsconfig.e2e.json",
            "devServerTarget": "stop-game-next:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "stop-game-next:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/stop-game-next-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "stop-game-api": {
      "root": "apps/stop-game-api",
      "sourceRoot": "apps/stop-game-api/src",
      "projectType": "application",
      "prefix": "stop-game-api",
      "targets": {
        "build": {
          "executor": "@nrwl/node:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/stop-game-api",
            "main": "apps/stop-game-api/src/main.ts",
            "tsConfig": "apps/stop-game-api/tsconfig.app.json",
            "assets": ["apps/stop-game-api/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/stop-game-api/src/environments/environment.ts",
                  "with": "apps/stop-game-api/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/node:execute",
          "options": { "buildTarget": "stop-game-api:build" }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": { "lintFilePatterns": ["apps/stop-game-api/**/*.ts"] }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/stop-game-api"],
          "options": {
            "jestConfig": "apps/stop-game-api/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "data": {
      "root": "libs/data",
      "sourceRoot": "libs/data/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": { "lintFilePatterns": ["libs/data/**/*.ts"] }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/data"],
          "options": {
            "jestConfig": "libs/data/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  }
}
